<h3>New Listing</h3>

<%= form_for @listing do |l| %>
  Address of Listing: <%= l.text_field :address %> <br />
  Price per Night: <%= l.number_field :price %> <br />
  About Listing: <%= l.text_field :content %> <br />
  Listing Pictures: <%= l.file_field :tours, multiple: true %>
  <%= l.submit %>
<% end %>